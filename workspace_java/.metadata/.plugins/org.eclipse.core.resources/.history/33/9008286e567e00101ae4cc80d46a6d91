<!-- html 주석 -->
<%-- jsp 주석 --%>

<!-- 디렉티브 테그 -->
<%--
	"<%@"로 시작하는 태그
	1. page
	2. include
 --%>
<%@ 
	page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8" import="java.util.List" import="java.util.Map"%>
<%-- import 찾는법 --%>
<%@page import="java.util.ArrayList"%>
<%@page import="java.util.*"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script>
	window.onload = function() {
		console.log("jsp");
	}
</script>
</head>
<body>
	<%@ include file="header.jsp"%>

	<h1>jsp.jsp</h1>

	<!-- 스크립틀릿Scriptlet -->
	<%
	//여기는 java 땅입니다
	System.out.println("test");
	out.println("<strong>허헣</strong>");
	%>

	<table border=1>
		<%
		for (int i = 0; i < 5; i++) {
		%>
		<tr>
			<td>제목</td>
			<td>내용</td>
		</tr>
		<%
		}
		%>
	</table>
	<%
		int a = 10;
	%>
	여기서 a : <% out.println(a); %>

</body>
</html>